{
    "jobDefinitionName": "",
    "type": "container",
    "parameters": {},
    "containerProperties": {
        "image": "gcr.io/cbb-research-dl/transannot",
        "vcpus": 64,
        "memory": 252000,
        "command": [
            "/usr/envs/transannot/bin/aws-pipeline.sh"
        ],
        "jobRoleArn": "",
        "volumes": [
            {
                "host": {
                    "sourcePath": "/data"
                },
                "name": "data"
            }
        ],
        "environment": [
            {
                "name": "BLAST_S3_BUCKET_SAMPLE",
                "value": "cbb-research-dl-blastdb"
            }
        ],
        "mountPoints": [
            {
                "containerPath": "/data",
                "sourceVolume": "data"
            }
        ],        
        "privileged": true,
        "ulimits": [],
        "user": "root"
    }
}
